<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HighKneesActivity">


    <TextView
        android:id="@+id/textView13"
        android:layout_width="301dp"
        android:layout_height="43dp"
        android:text="          High Knees"
        android:background="@color/colorPrimaryDark"
        android:textColor="#fff"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="405dp"
        android:layout_height="303dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:layout_constraintVertical_bias="0.121"
        app:srcCompat="@drawable/highknees" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="366dp"
        android:layout_height="203dp"
        android:textSize="19dp"
        android:textAlignment="center"
        android:text="It helps you work on inner thighs and outer hip areas. Itâ€™s good belly fat burning exercise too!

How to do: stand straight with feet hip-width apart. Raise your right knee as high as possible while raise your left arm. Reverse the move. Continue pulling knees up quickly."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.644"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:layout_constraintVertical_bias="0.377"
        android:gravity="center_horizontal" />


</androidx.constraintlayout.widget.ConstraintLayout>